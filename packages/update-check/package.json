		"semver": "7.6.3"
	},
	"devDependencies": {
		"@directus/tsconfig": "workspace:*",
		"@npm/types": "1.0.2",
		"@types/node": "18.19.50",
		"@types/semver": "7.5.8",
		"@vitest/coverage-v8": "1.5.3",
		"strip-ansi": "7.1.0",
		"tsup": "8.2.4",
		"typescript": "5.4.5",
		"vitest": "1.5.3"
	}
