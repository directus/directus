		"@directus/tsconfig": "3.0.0",
		"@directus/types": "workspace:*",
		"@types/lodash-es": "4.17.12",
		"@vitest/coverage-v8": "2.1.8",
		"@vue/test-utils": "2.4.6",
		"tsup": "8.3.5",
		"typescript": "5.7.3",
