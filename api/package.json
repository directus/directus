		"./package.json": "./package.json"
	},
	"bin": {
		"directus": "cli.js"
	},
	"scripts": {
		"start": "npx directus start",
		"prebuild": "pnpm cleanup",
		"build": "tsc --build && copyfiles \"src/**/*.*\" -e \"src/**/*.ts\" -u 1 dist",
		"cleanup": "rimraf dist",
		"dev": "cross-env NODE_ENV=development SERVE_APP=false tsx watch ./src/start.ts",
		"cli": "cross-env NODE_ENV=development SERVE_APP=false tsx watch ./src/cli/run.ts",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:watch": "vitest"
	},
